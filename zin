 const formik = useFormik({
    initialValues,
    validationSchema: Yup.object({
      name: Yup.string()
        .max(25, 'Must be 25 characters or less')
        .required('Required'),
      description: Yup.string().required('Required'),
      price: Yup.number().required('required'),
      brief: Yup.string(),
      category: Yup.array(),
      tags: Yup.array(),
    }),
    onSubmit: (values) => {
      alert(JSON.stringify(values, null, 2));
    },
  });